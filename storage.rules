
rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // Users can only read and write their own files, which should be stored
    // in a path that includes their user ID.
    match /users/{userId}/{allPaths=**} {
      allow read, write: if request.auth != null && request.auth.uid == userId;
    }
  }
}
